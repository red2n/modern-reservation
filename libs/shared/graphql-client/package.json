{
  "name": "@modern-reservation/graphql-client",
  "version": "1.0.0",
  "description": "Shared GraphQL client for Modern Reservation System",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "check": "biome check .",
    "check:fix": "biome check --write ."
  },
  "keywords": [
    "graphql",
    "apollo-client",
    "modern-reservation"
  ],
  "author": "Modern Reservation Team",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^4.0.7",
    "graphql": "^16.10.0"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@types/node": "^20.0.0",
    "@types/react": "^19",
    "typescript": "^5.7.3"
  }
}
